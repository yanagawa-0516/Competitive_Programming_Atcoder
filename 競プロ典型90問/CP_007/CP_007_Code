#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
using ll=long long;

int main(){
  int N;
  cin>>N;
  vector<ll> A(N);
  for(int i=0;i<N;i++) cin>>A[i];
  sort(A.begin(),A.end());
  
  int Q;
  cin>>Q;
  
  for(int j=0;j<Q;j++){
    ll B;
    cin>>B;
    
    int left=0,right=N-1;
    while(right-left>1){
      int mid=(left+right)/2;
      if(A[mid]<B) left=mid;
      else right=mid;
    }
    
    ll ans=min(abs(B-A[left]),abs(A[right]-B));
    cout<<ans<<endl;
  }
  
  return 0;
}
