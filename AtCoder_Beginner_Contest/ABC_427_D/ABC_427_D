#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main(){
  int T;
  cin>>T;
  
  for(int iter=0;iter<T;iter++){
    int N,M,K;
    string S;
    cin>>N>>M>>K>>S;
    
    vector<vector<int>> graph(N+1);
    for(int i=0;i<M;i++){
      int u,v;
      cin>>u>>v;
      graph[u].push_back(v);
    }
    
    vector<vector<char>> dp(N+1,vector<char>(2*K+1,' '));
    
    for(int u=1;u<=N;u++){
      dp[u][2*K]=S[u-1];
    }
    
    for(int t=2*K-1;t>=0;t--){
      for(int u=1;u<=N;u++){
        if(t%2==0){
          char a='B';
          for(int v:graph[u]){
            if(dp[v][t+1]=='A'){
              a='A';
              break;
            }
          }
          dp[u][t]=a;
        }
        else{
          char a='A';
          for(int v:graph[u]){
            if(dp[v][t+1]=='B'){
              a='B';
              break;
            }
          }
          dp[u][t]=a;
        }
      }
    }
    
    if(dp[1][0]=='A') cout<<"Alice"<<endl;
    else cout<<"Bob"<<endl;
  }
  
  return 0;
}
